{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/jquery/",
      "files": [
        "dist/jquery.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "apexcharts@3.23.1",
      "destination": "wwwroot/lib/apexcharts/",
      "files": [
        "apexcharts.min.js",
        "apexcharts.min.css"
      ]
    }
  ]
}